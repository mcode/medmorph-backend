{
  "name": "medmorph-backend",
  "version": "0.0.1",
  "author": "The MITRE Corporation",
  "license": "Apache-2.0",
  "description": "MedMorph Backend Service Client",
  "scripts": {
    "start": "npm-run-all --parallel startFrontend startBackend",
    "startProd": "WATCH=false npm-run-all -s startFrontend startBackend",
    "startBackend": "nodemon ./bin/www",
    "startFrontend": "webpack",
    "debug": "node --inspect-brk ./bin/www",
    "add-server-js": "node ./bin/add-server",
    "add-server-sh": "./bin/add-server.sh",
    "test": "jest",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "lint": "eslint './src/**/*.{js,jsx}'",
    "lint:fix": "eslint './src/**/*.{js,jsx}' --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "babel-loader": "^8.0.5",
    "css-loader": "^5.0.2",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-only-error": "^1.0.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "1.19.1",
    "style-loader": "^2.0.0",
    "supertest": "^6.1.3",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "babel-eslint": "^10.1.0",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "eslint": "^7.19.0",
    "express": "^4.17.1",
    "fhir": "^4.8.2",
    "graphlib": "^2.1.8",
    "http-status-codes": "^2.1.4",
    "lokijs": "^1.5.11",
    "morgan": "~1.9.1",
    "node-jose": "^2.0.0",
    "nodemon": "^2.0.7",
    "passport": "^0.4.1",
    "passport-keycloak-bearer": "^2.0.5",
    "query-string": "^6.13.8",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-transition-group": "^4.3.0",
    "uuid": "^8.3.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
